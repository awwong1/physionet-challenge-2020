{
  "exp_name": "PhysioNet2020/SimpleCNN",
  "agent": "agents.physionet2020.ClassificationAgent",
  "dataset": {
      "name": "datasets.physionet2020.PhysioNet2020Dataset",
      "kwargs": {
        "data_dir": "Training_WFDB",
        "fs": 500,
        "max_seq_len": 4000,
        "proc": 8,
        "ensure_equal_len": true,
        "signal_last": true
      }
  },
  "val_split": 0.1,
  "train_loader": {
      "batch_size": 256,
      "num_workers": 80,
      "shuffle": true,
      "pin_memory": true
  },
  "val_loader": {
      "batch_size": 128,
      "num_workers": 16,
      "shuffle": false,
      "pin_memory": true
  },
  "model": {
    "name": "models.simple_cnn.SimpleCNN"
  },
  "criterion": {
    "name": "torch.nn.BCEWithLogitsLoss"
  },
  "optimizer": {
    "name": "torch.optim.Adam",
    "kwargs": {
      "lr": 0.05
    }
  },
  "scheduler": {
    "name": "torch.optim.lr_scheduler.StepLR",
    "kwargs": {
      "step_size": 2,
      "gamma": 0.2
    }
  },
  "epochs": 10,
  "log_per_num_batch": 3
}

