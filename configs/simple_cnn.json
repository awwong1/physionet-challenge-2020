{
  "exp_name": "PhysioNet2020/SimpleCNN",
  "agent": "agents.physionet2020.ClassificationAgent",
  "dataset": {
      "name": "datasets.processed.ProcessedDataset",
      "args": ["data/fs500-msl4000"]
  },
  "val_split": 0.1,
  "train_loader": {
      "batch_size": 128,
      "num_workers": 16,
      "shuffle": true,
      "pin_memory": true
  },
  "val_loader": {
      "batch_size": 64,
      "num_workers": 16,
      "shuffle": false,
      "pin_memory": true
  },
  "model": {
    "name": "models.simple_cnn.SimpleCNN"
  },
  "criterion": {
    "name": "torch.nn.BCEWithLogitsLoss"
  },
  "optimizer": {
    "name": "torch.optim.Adam",
    "kwargs": {
      "lr": 0.01
    }
  },
  "scheduler": {
    "name": "torch.optim.lr_scheduler.StepLR",
    "kwargs": {
      "step_size": 6,
      "gamma": 0.1
    }
  },
  "epochs": 30,
  "log_per_num_batch": 3,
  "use_amp": true
}

