{
  "exp_name": "PhysioNet2020/SignalFourierTransformLSTM/cv5-0",
  "agent": "agents.physionet2020.ClassificationAgent",
  "dp_batch_dim": 1,
  "use_collate_fn": true,
  "dataset": {
    "name": "datasets.physionet2020.PhysioNet2020Dataset",
    "kwargs": {
      "data_dir": "Training_WFDB",
      "max_seq_len": 4000,
      "derive_fft": true
    }
  },
  "cross_validation": {
    "fold": 5,
    "val_offset": 0
  },
  "train_loader": {
      "batch_size": 128,
      "num_workers": 16,
      "shuffle": true,
      "pin_memory": true
  },
  "val_loader": {
      "batch_size": 128,
      "num_workers": 16,
      "shuffle": false,
      "pin_memory": true
  },
  "model": {
    "name": "models.sig_fft_lstm.SignalFourierTransformLSTM"
  },
  "criterion": {
    "name": "torch.nn.BCEWithLogitsLoss"
  },
  "optimizer": {
    "name": "apex.optimizers.FusedAdam",
    "kwargs": {
      "lr": 0.01
    }
  },
  "scheduler": {
    "name": "torch.optim.lr_scheduler.ReduceLROnPlateau",
    "kwargs": {
      "mode": "min",
      "factor": 0.2,
      "patience": 4
    }
  },
  "epochs": 30,
  "log_per_num_batch": 1,
  "use_amp": true
}

